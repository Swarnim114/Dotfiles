{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f0f2390",
                "editor.background": "#0f0f2390",
                "editor.foreground": "#c3c3c8",
                "text": "#c3c3c8",
                "text.muted": "#c3c3c870",
                "text.ignored": "#c3c3c840",
                "text.placeholder": "#c3c3c850",
                "ignored": "#c3c3c830",
                "element.hover": "#c3c3c830",
                "ghost_element.hover": "#62677B30",
                "ghost_element.selected": "#62677B30",
                "ghost_element.active": "#62677B60",
                "border": "#0f0f23",
                "editor.highlighted_line.background": "#62677B10",
                "editor.active_line.background": "#62677B10",
                "panel.background": "#0f0f2390",
                "title_bar.background": "#0f0f2390",
                "title_bar.inactive_background": "#0f0f2390",
                "status_bar.background": "#0f0f2390",
                "drop_target.background": "#0f0f2390",
                "elevated_surface.background": "#0f0f23",
                "toolbar.background": "#0f0f2390",
                "tab_bar.background": "#0f0f2390",
                "tab.inactive_background": "#0f0f2390",
                "tab.active_background": "#62677B30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f0f23",
                "scrollbar.thumb.background": "#62677B",
                "editor.gutter.background": "#0f0f2390",
                "terminal.background": "#0f0f2310",
                "terminal.foreground": "#c3c3c8",
                "terminal.dim_foreground": "#c3c3c8",
                "terminal.bright_foreground": "#c3c3c8",
                "terminal.ansi.black": "#0f0f23",
                "terminal.ansi.red": "#62677B",
                "terminal.ansi.green": "#6F758A",
                "terminal.ansi.yellow": "#857C90",
                "terminal.ansi.blue": "#7C849D",
                "terminal.ansi.magenta": "#8993AF",
                "terminal.ansi.cyan": "#9FAFD8",
                "terminal.ansi.white": "#c3c3c8",
                "terminal.ansi.bright_black": "#5e5e74",
                "terminal.ansi.bright_red": "#62677B",
                "terminal.ansi.bright_green": "#6F758A",
                "terminal.ansi.bright_yellow": "#857C90",
                "terminal.ansi.bright_blue": "#7C849D",
                "terminal.ansi.bright_magenta": "#8993AF",
                "terminal.ansi.bright_cyan": "#9FAFD8",
                "terminal.ansi.bright_white": "#c3c3c8",
                "modified": "#62677B",
                "syntax": {
                    "attribute": {
                        "color": "#62677B"
                    },
                    "boolean": {
                      "color": "#6F758A"
                    },
                    "comment": {
                      "color": "#5e5e74"
                    },
                    "comment.doc": {
                      "color": "#5e5e74"
                    },
                    "constant": {
                      "color": "#6F758A"
                    },
                    "function": {
                      "color": "#9FAFD8"
                    },
                    "keyword": {
                      "color": "#7C849D"
                    },
                    "number": {
                      "color": "#8993AF"
                    },
                    "operator": {
                      "color": "#7C849D"
                    },
                    "string": {
                      "color": "#62677B"
                    },
                    "variable": {
                      "color": "#6F758A"
                    }
                },
                "players": [
                  {
                    "cursor": "#62677B",
                    "background": "#0f0f23",
                    "selection": "#62677B30"
                  }
                ]
            }
        }
    ]
}
