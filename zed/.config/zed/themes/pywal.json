{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#10121290",
                "editor.background": "#10121290",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#77883930",
                "ghost_element.selected": "#77883930",
                "ghost_element.active": "#77883960",
                "border": "#101212",
                "editor.highlighted_line.background": "#77883910",
                "editor.active_line.background": "#77883910",
                "panel.background": "#10121290",
                "title_bar.background": "#10121290",
                "title_bar.inactive_background": "#10121290",
                "status_bar.background": "#10121290",
                "drop_target.background": "#10121290",
                "elevated_surface.background": "#101212",
                "toolbar.background": "#10121290",
                "tab_bar.background": "#10121290",
                "tab.inactive_background": "#10121290",
                "tab.active_background": "#77883930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#101212",
                "scrollbar.thumb.background": "#778839",
                "editor.gutter.background": "#10121290",
                "terminal.background": "#10121210",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#101212",
                "terminal.ansi.red": "#778839",
                "terminal.ansi.green": "#6C965F",
                "terminal.ansi.yellow": "#B4B350",
                "terminal.ansi.blue": "#96A460",
                "terminal.ansi.magenta": "#658388",
                "terminal.ansi.cyan": "#B0B493",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#596d6d",
                "terminal.ansi.bright_red": "#778839",
                "terminal.ansi.bright_green": "#6C965F",
                "terminal.ansi.bright_yellow": "#B4B350",
                "terminal.ansi.bright_blue": "#96A460",
                "terminal.ansi.bright_magenta": "#658388",
                "terminal.ansi.bright_cyan": "#B0B493",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#778839",
                "syntax": {
                    "attribute": {
                        "color": "#778839"
                    },
                    "boolean": {
                      "color": "#6C965F"
                    },
                    "comment": {
                      "color": "#596d6d"
                    },
                    "comment.doc": {
                      "color": "#596d6d"
                    },
                    "constant": {
                      "color": "#6C965F"
                    },
                    "function": {
                      "color": "#B0B493"
                    },
                    "keyword": {
                      "color": "#96A460"
                    },
                    "number": {
                      "color": "#658388"
                    },
                    "operator": {
                      "color": "#96A460"
                    },
                    "string": {
                      "color": "#778839"
                    },
                    "variable": {
                      "color": "#6C965F"
                    }
                },
                "players": [
                  {
                    "cursor": "#778839",
                    "background": "#101212",
                    "selection": "#77883930"
                  }
                ]
            }
        }
    ]
}
