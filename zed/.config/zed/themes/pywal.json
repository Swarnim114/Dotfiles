{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#170f0a90",
                "editor.background": "#170f0a90",
                "editor.foreground": "#c5c3c1",
                "text": "#c5c3c1",
                "text.muted": "#c5c3c170",
                "text.ignored": "#c5c3c140",
                "text.placeholder": "#c5c3c150",
                "ignored": "#c5c3c130",
                "element.hover": "#c5c3c130",
                "ghost_element.hover": "#70767430",
                "ghost_element.selected": "#70767430",
                "ghost_element.active": "#70767460",
                "border": "#170f0a",
                "editor.highlighted_line.background": "#70767410",
                "editor.active_line.background": "#70767410",
                "panel.background": "#170f0a90",
                "title_bar.background": "#170f0a90",
                "title_bar.inactive_background": "#170f0a90",
                "status_bar.background": "#170f0a90",
                "drop_target.background": "#170f0a90",
                "elevated_surface.background": "#170f0a",
                "toolbar.background": "#170f0a90",
                "tab_bar.background": "#170f0a90",
                "tab.inactive_background": "#170f0a90",
                "tab.active_background": "#70767430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#170f0a",
                "scrollbar.thumb.background": "#707674",
                "editor.gutter.background": "#170f0a90",
                "terminal.background": "#170f0a10",
                "terminal.foreground": "#c5c3c1",
                "terminal.dim_foreground": "#c5c3c1",
                "terminal.bright_foreground": "#c5c3c1",
                "terminal.ansi.black": "#170f0a",
                "terminal.ansi.red": "#707674",
                "terminal.ansi.green": "#9A6D59",
                "terminal.ansi.yellow": "#BE7348",
                "terminal.ansi.blue": "#7D817C",
                "terminal.ansi.magenta": "#828279",
                "terminal.ansi.cyan": "#B48366",
                "terminal.ansi.white": "#c5c3c1",
                "terminal.ansi.bright_black": "#6d6259",
                "terminal.ansi.bright_red": "#707674",
                "terminal.ansi.bright_green": "#9A6D59",
                "terminal.ansi.bright_yellow": "#BE7348",
                "terminal.ansi.bright_blue": "#7D817C",
                "terminal.ansi.bright_magenta": "#828279",
                "terminal.ansi.bright_cyan": "#B48366",
                "terminal.ansi.bright_white": "#c5c3c1",
                "modified": "#707674",
                "syntax": {
                    "attribute": {
                        "color": "#707674"
                    },
                    "boolean": {
                      "color": "#9A6D59"
                    },
                    "comment": {
                      "color": "#6d6259"
                    },
                    "comment.doc": {
                      "color": "#6d6259"
                    },
                    "constant": {
                      "color": "#9A6D59"
                    },
                    "function": {
                      "color": "#B48366"
                    },
                    "keyword": {
                      "color": "#7D817C"
                    },
                    "number": {
                      "color": "#828279"
                    },
                    "operator": {
                      "color": "#7D817C"
                    },
                    "string": {
                      "color": "#707674"
                    },
                    "variable": {
                      "color": "#9A6D59"
                    }
                },
                "players": [
                  {
                    "cursor": "#707674",
                    "background": "#170f0a",
                    "selection": "#70767430"
                  }
                ]
            }
        }
    ]
}
